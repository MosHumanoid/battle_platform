<launch>

  <group ns="r1">
    <include file="$(find bitbots_bringup)/launch/load_robot_description_thmos.launch"/>
    <rosparam file="$(find bitbots_quintic_walk)/config/walking_wolfgang_viz.yaml" command="load"/>
    <node name="walking" pkg="bitbots_quintic_walk" type="WalkNode" output="screen"/>

    <!-- <arg name="tf_prefix" value="r1/"/> -->
    <node name="joint_goal_viz" pkg="bitbots_bringup" type="motor_goals_viz_helper_thmos.py" output="screen" args="--walking"/>
    <node pkg="rviz" type="rviz" args="-d $(find bitbots_quintic_walk)/config/walk.rviz" name="walk_rviz"/>
  </group>

  <group ns="b1">
    <include file="$(find bitbots_bringup)/launch/load_robot_description_thmos.launch"/>
    <rosparam file="$(find bitbots_quintic_walk)/config/walking_wolfgang_viz.yaml" command="load"/>
    <node name="walking" pkg="bitbots_quintic_walk" type="WalkNode" output="screen"/>

    <!-- <arg name="tf_prefix" value="b1/"/> -->
    <node name="joint_goal_viz" pkg="bitbots_bringup" type="motor_goals_viz_helper_thmos.py" output="screen" args="--walking"/>
    <node pkg="rviz" type="rviz" args="-d $(find bitbots_quintic_walk)/config/walk.rviz" name="walk_rviz"/>
  </group>

</launch>
